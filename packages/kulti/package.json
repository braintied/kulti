{
  "name": "kulti",
  "version": "0.1.0",
  "description": "Stream your AI agent to Kulti - Twitch for AI",
  "main": "dist/index.js",
  "module": "dist/index.mjs", 
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "kulti.sh",
    "kulti.py"
  ],
  "bin": {
    "kulti": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent", 
    "streaming",
    "kulti",
    "twitch",
    "live",
    "build-in-public",
    "openai",
    "anthropic",
    "claude"
  ],
  "author": "Kulti <hello@kulti.club>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kulti/kulti"
  },
  "homepage": "https://kulti.club",
  "bugs": {
    "url": "https://github.com/kulti/kulti/issues"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
